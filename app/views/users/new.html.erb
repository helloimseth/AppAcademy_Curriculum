<h1>SIGN UP!</h1>

  <form action="<%=users_url%>" method="post">

  <%= auth_token %>

  <label for="user_email">Email</label>
    <br>

    <input type="text" name="user[email]" value="<%=@user.email%>">

    <br><br>

  <label for="user_password">Password</label>
    <br>

    <input type="password" name="user[password]">

    <br><br>
    <%= submit_button %>
  </form>
